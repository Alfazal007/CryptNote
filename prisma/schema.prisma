generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  secrets Secret[]
}

model Secret {
  id    Int    @id @default(autoincrement())
  key   String
  value String
  userId Int
  user User @relation(fields: [userId], references: [id])
  @@unique([userId, key])
}
